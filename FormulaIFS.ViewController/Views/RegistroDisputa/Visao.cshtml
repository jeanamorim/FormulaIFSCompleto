@model IEnumerable<FormulaIFS.Model.RegistroDisputa>

@{
    ViewBag.Title = "Visao";
    Layout = null;
}

<table class="table table-striped table-condensed" id="RegistroDisputaTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DataRegistro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EhTreino)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sequencial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tempo)
            </th>

            <th></th>
        </tr>

    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.DataRegistro)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EhTreino)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Sequencial)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Tempo)
                </td>
                <td>
                    <a class="btn btn-primary btn-sm" onclick="Edit('@Url.Action("AdicionarEditar", "RegistroDisputa", new { id=@item.Id})')"><i class="fa fa-pencil fa-lg"></i></a>
                    <a class="btn btn-danger btn-sm" onclick="Delete('@Url.Action("Delete", "RegistroDisputa", new { id=@item.Id})')"><i class="fa fa-trash fa-lg"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>